<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhancheng.dao.UserMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.zhancheng.entity.User" id="userMap">
        <result property="uid" column="uid"/>
        <result property="username" column="username"/>
        <result property="pwd" column="pwd"/>
        <result property="phone" column="phone"/>
        <result property="openid" column="openid"/>
        <result property="nickname" column="nickname"/>
        <result property="signature" column="signature"/>
        <result property="birth" column="birth"/>
        <result property="gender" column="gender"/>
        <result property="email" column="email"/>
        <result property="cover" column="cover"/>
        <result property="status" column="status"/>
        <result property="jmsgName" column="jmsg_name"/>
        <result property="jmsgPwd" column="jmsg_pwd"/>
        <result property="isDeleted" column="is_deleted"/>
        <result property="gmtCreate" column="gmt_create"/>
        <result property="gmtModified" column="gmt_modified"/>
        <result property="gmtLogin" column="gmt_login"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uid, username, pwd, phone, openid, nickname, signature, birth, gender, email, cover, status, jmsg_name, jmsg_pwd,
        is_deleted, gmt_create, gmt_modified, gmt_login,
    </sql>


</mapper>